<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net8.0</TargetFramework>
    <PackAsTool>true</PackAsTool>
    <DisableImplicitFSharpCoreReference>true</DisableImplicitFSharpCoreReference>
    <Authors>Aardvark Platform Team</Authors>
    <Description>.NET tool for packing Aardvark Platform projects.</Description>
    <OutputPath>..\..\bin\$(Configuration)</OutputPath>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="Program.fs" />
    <None Include="paket.references" />
  </ItemGroup>
  <Import Project="..\..\bin\Release\build\Aardvark.Build.targets" Condition="'$(Configuration)' == 'Release' AND Exists('..\..\bin\Release\build\Aardvark.Build.targets')" />
  <Import Project="..\..\.paket\Paket.Restore.targets" />
</Project>